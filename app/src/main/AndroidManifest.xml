<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.welling.kinghacker.activities">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/helper"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.welling.kinghacker.activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.welling.kinghacker.activities.HomeActivity"></activity>
        <activity android:name="com.welling.kinghacker.activities.BloodSugerActivity"></activity>
        <activity android:name=".ChangePasswordActivity"></activity>
        <activity android:name=".AboutActivity"></activity>
        <activity android:name=".SettingActivity"></activity>
        <activity android:name=".InformationActivity"></activity>
        <activity android:name=".BloodPressureActivity"></activity>
        <activity android:name=".BloodOxygenActivity"></activity>
    </application>

</manifest>
